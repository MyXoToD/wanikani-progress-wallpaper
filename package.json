{
  "name": "wanikani-progress-wallpaper",
  "version": "1.0.0",
  "description": "Simple web app to generate a progress wallpaper of your wanikani progress",
  "type": "module",
  "main": "src/app.ts",
  "scripts": {
    "start": "nodemon --exec tsx ./src/app.ts",
    "start1": "ts-node src/app.ts",
    "build": "tsc",
    "serve": "node dist/app.js",
    "lint": "npx eslint src",
    "format": "prettier --ignore-path .gitignore --write \"**/*.+(js|ts|json)\"",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MyXoToD/wanikani-progress-wallpaper.git"
  },
  "keywords": [
    "wanikani",
    "wallpaper",
    "srs",
    "statistics"
  ],
  "author": "Maximilian Boll",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/MyXoToD/wanikani-progress-wallpaper/issues"
  },
  "homepage": "https://github.com/MyXoToD/wanikani-progress-wallpaper#readme",
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "^8.46.1",
    "body-parser": "^2.2.0",
    "dotenv": "^17.2.3",
    "express": "^5.1.0",
    "pug": "^3.0.3"
  },
  "devDependencies": {
    "@types/express": "^5.0.3",
    "eslint": "^9.37.0",
    "eslint-config-prettier": "^10.1.8",
    "globals": "^16.4.0",
    "jest": "^30.2.0",
    "nodemon": "^3.1.10",
    "prettier": "^3.6.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.46.1"
  }
}
